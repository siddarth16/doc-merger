# Doc Merger Environment Variables

# Document Conversion API (Optional)
# Enable this for mixed PDF/DOCX merging
# CONVERT_API_PROVIDER=cloudconvert
# CONVERT_API_KEY=your_cloudconvert_api_key_here

# Alternative: ConvertAPI
# CONVERT_API_PROVIDER=convertapi
# CONVERT_API_KEY=your_convertapi_secret_here

# Analytics (Optional, disabled by default)
# NEXT_PUBLIC_ANALYTICS=false

# App Configuration
# NEXT_PUBLIC_MAX_FILE_SIZE=50
# NEXT_PUBLIC_MAX_FILES=10

# Feature Flags
# NEXT_PUBLIC_ENABLE_CONVERSION=false
# NEXT_PUBLIC_ENABLE_OCR=false
